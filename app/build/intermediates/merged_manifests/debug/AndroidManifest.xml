<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myrestfulmind"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.MyRestfulMind" >
        <activity
            android:name="com.example.myrestfulmind.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myrestfulmind.ClientloginActivity" />
        <activity android:name="com.example.myrestfulmind.TherapistloginActivity" />
        <activity android:name="com.example.myrestfulmind.SignupActivity" />
        <activity android:name="com.example.myrestfulmind.TherapistsignupActivity" />
        <activity android:name="com.example.myrestfulmind.ClienthomeActivity" />
        <activity android:name="com.example.myrestfulmind.TherapisthomeActivity" />
        <activity android:name="com.example.myrestfulmind.surveyOneActivity" />
        <activity android:name="com.example.myrestfulmind.surveyTwoActivity" />
        <activity android:name="com.example.myrestfulmind.surveyThreeActivity" />
        <activity android:name="com.example.myrestfulmind.surveyFourActivity" />
        <activity android:name="com.example.myrestfulmind.surveyFiveActivity" />
        <activity android:name="com.example.myrestfulmind.AboutUsActivity" />
        <activity android:name="com.example.myrestfulmind.FaqsActivity" />
        <activity android:name="com.example.myrestfulmind.TermsActivity" />
        <activity android:name="com.example.myrestfulmind.CalenderActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name="androidx.room.MultiInstanceInvalidationService"
            android:directBootAware="true"
            android:exported="false" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.myrestfulmind.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>